{
    "name": "@doist/ui-extensions-react",
    "version": "17.0.0",
    "author": "Doist",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist",
        "styles"
    ],
    "repository": "https://github.com/Doist/ui-extensions",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "watch": "npm run build && yalc push && chokidar src -c \"npm run build && yalc push\"",
        "build": "npm run clean && npx rollup -c && ./scripts/reorganize-styles.sh",
        "test": "jest --passWithNoTests",
        "lint-fix": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
        "lint-check": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
        "format-check": "npx prettier --check .",
        "format-fix": "npx prettier --write .",
        "ts-compile-check": "tsc --noEmit",
        "clean": "rimraf dist styles",
        "reset": "rimraf dist styles node_modules",
        "prepare": "npm run build",
        "storybook": "storybook dev -p 6006 -s ../src/stories/public",
        "build-storybook": "storybook build",
        "integrity-check": "npm-run-all clean format-check lint-check ts-compile-check test",
        "link:yalc": "yalc link",
        "publish:yalc": "yalc push"
    },
    "peerDependencies": {
        "@doist/reactist": "^29.0.0",
        "@doist/ui-extensions-core": "^5.0.0",
        "adaptivecards": "^2.9.0",
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0",
        "react-select": "5.10.2"
    },
    "prettier": "@doist/prettier-config",
    "dependencies": {
        "classnames": "2.5.1",
        "dayjs": "1.11.19"
    },
    "devDependencies": {
        "@babel/preset-env": "7.28.5",
        "@babel/preset-react": "7.28.5",
        "@babel/preset-typescript": "7.28.5",
        "@doist/reactist": "29.1.1",
        "@storybook/addon-actions": "7.6.21",
        "@storybook/addon-essentials": "7.6.21",
        "@storybook/addon-links": "7.6.21",
        "@storybook/react": "7.6.21",
        "@storybook/react-webpack5": "7.6.21",
        "@svgr/rollup": "8.1.0",
        "@testing-library/dom": "10.4.1",
        "@testing-library/jest-dom": "5.17.0",
        "@testing-library/react": "9.5.0",
        "@testing-library/react-hooks": "3.7.0",
        "@types/react": "18.3.26",
        "graphql": "16.12.0",
        "identity-obj-proxy": "3.0.0",
        "msw": "2.12.7",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-is": "17.0.2",
        "react-test-renderer": "17.0.2",
        "storybook": "7.6.21"
    },
    "msw": {
        "workerDirectory": "src/stories/public"
    }
}
